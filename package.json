{
  "name": "cover-mba",
  "version": "0.2.1",
  "description": "Библиотека для отслеживания покрытия API тестами с использованием Playwright",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "npx playwright test",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "format": "prettier --write src/**/*.js tests/**/*.js"
  },
  "keywords": [
    "api",
    "coverage",
    "testing",
    "playwright",
    "swagger",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.10",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "swagger-parser": "^2.1.26"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "eslint": "^8.56.0",
    "playwright": "^1.52.0",
    "prettier": "^3.2.5"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.ejs"
  ]
}
