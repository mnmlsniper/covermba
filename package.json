{
  "name": "cover-mba",
  "version": "0.2.0",
  "description": "Библиотека для отслеживания покрытия API тестами с использованием Playwright",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node --test tests/*.test.js",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "format": "prettier --write src/**/*.js tests/**/*.js"
  },
  "keywords": [
    "api",
    "coverage",
    "testing",
    "playwright",
    "swagger",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "ejs": "^3.1.10",
    "swagger-parser": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.ejs"
  ]
}
